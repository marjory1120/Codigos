function setup() {
  createCanvas(400, 400);
}

function draw() {
  background("black");
  fill("rgb(252,199,181)")
  circle(200,200,300);
  fill("white")
  circle(150,150,60);
  circle(250,150,60);
  line(150,270,250,235);
  fill("rgb(245,205,185)")
  triangle(200,180,180,220,220,220);
  line(123,115,178,113)
  line(225,116,279,113)
  circle(150)
  
  if (mouseIsPressed) {
    console.log(mouseX, mouseY);
  }
}
